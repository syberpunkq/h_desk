<h1>Users list</h1>
<table>
  <thead>
    <tr>
      <th>Имя</th>
      <th>Логин</th>
      <th>Пароль</th>
      <th>Email</th>
    </thead>
  <tbody>
    
     
    
    
  
<% @users.each do |user| %>
<tr>
<td><%= user.name %></td>
<td><%= user.login %></td>
<td><%= user.password %></td>
<td><%= user.email %></td>
</tr>
<% end %>
  </tbody>
</table>